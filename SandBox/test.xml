<assets>
    <textures/>
    <fonts/>
    <areas/>
    <sprites/>
    <backgrounds/>
    <animations>
	
        <animation name="rot" id="4">
            <properties time_length="3" loop="true" reverse="false"/>
            <begin_transform>
                <position x="0" y="0"/>
                <rotation angle="0"/>
                <scale x="1" y="1"/>
            </begin_transform>
            <position_track>
                <key time="0" x="0" y="0"/>
                <key time="1" x="100" y="200"/>
                <key time="3" x="0" y="0"/>
            </position_track>
			 <sprite_frame_track>
                <key time="0" frame="0"/>
                <key time="1" frame="1"/>
                <key time="2" frame="2"/>
                <key time="3" frame="1"/>
            </sprite_frame_track>
            <rotation_track>
                <key time="0" angle="0"/>
                <key time="1" angle="180"/>
                <key time="2" angle="-20"/>
                <key time="3" angle="0"/>
            </rotation_track>
            <scale_track>
                <key time="0" x="1" y="1"/>
                <key time="1" x="2" y="0.5"/>
                <key time="3" x="1" y="1"/>
            </scale_track>
        </animation>
    </animations>
</assets>
